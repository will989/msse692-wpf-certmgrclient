<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
  <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>
  <log4net>
    <!--
  This stores information in the mylogfile.txt file.  
  -->
    <appender name="RollingFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="C:\Temp\mylogfile.txt"/>
      <appendToFile value="true"/>
      <rollingStyle value="Size"/>
      <maxSizeRollBackups value="5"/>
      <maximumFileSize value="10MB"/>
      <staticLogFileName value="true"/>
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %level %logger - %message%newline%exception"/>
      </layout>
    </appender>
    <root>
      <level value="DEBUG"/>
      <appender-ref ref="RollingFileAppender"/>
    </root>
   </log4net>
    <system.serviceModel>
        <bindings>
            <basicHttpBinding>
                <binding name="CertificateWarehouseService" />
            </basicHttpBinding>
            <wsHttpBinding>
                <binding name="WSHttpBinding_ICertificate" />
            </wsHttpBinding>
        </bindings>
        <client>
            <endpoint address="http://win-iv85psjvcob/CertificateWarehouseService.svc"
                binding="basicHttpBinding" bindingConfiguration="CertificateWarehouseService"
                contract="CertificateWarehouseService.ICertificateWarehouseService"
                name="CertificateWarehouseService" />
            <endpoint address="http://win-iv85psjvcob:8000/ServiceModelSamples/service"
                binding="wsHttpBinding" bindingConfiguration="WSHttpBinding_ICertificate"
                contract="CertificateService.ICertificate" name="WSHttpBinding_ICertificate">
                <identity>
                    <userPrincipalName value="Administrator@williams.local" />
                </identity>
            </endpoint>
        </client>
    </system.serviceModel>
<startup><supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/></startup></configuration>
